{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["tsconfig.base.json", ".eslintrc.js"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "out/**", ".next/**", "tsconfig.tsbuildinfo"]
    },
    "depcheck": {
      "dependsOn": [],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "format": {
      "cache": false
    },
    "lint": {
      "outputs": [".eslintcache"],
      "outputLogs": "new-only"
    },
    "lint:commit": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": [],
      "outputLogs": "new-only"
    }
  }
}
