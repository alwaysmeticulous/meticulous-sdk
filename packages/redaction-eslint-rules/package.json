{
  "name": "@alwaysmeticulous/redaction-eslint-rules",
  "version": "1.0.0",
  "license": "ISC",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "dev": "parcel watch",
    "build": "parcel build",
    "format": "prettier --write src",
    "lint": "eslint src --ext=ts,tsx,js --cache",
    "lint:commit": "eslint --cache $(git diff --relative --name-only --diff-filter=ACMRTUXB master | grep  -E \"(.js$|.ts$|.tsx$)\")",
    "lint:fix": "eslint src --ext=ts,tsx,js --cache --fix",
    "depcheck": "depcheck --ignore-patterns=dist",
    "test": "jest"
  },
  "dependencies": {
    "@alwaysmeticulous/redaction": "^2.187.0",
    "@typescript-eslint/utils": "8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/rule-tester": "8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-plugin-eslint-plugin": "^6.3.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "8.18.0",
    "minimatch": "^10.0.1"
  },
  "author": {
    "name": "The Meticulous Team",
    "email": "eng@meticulous.ai",
    "url": "https://meticulous.ai"
  },
  "homepage": "https://github.com/alwaysmeticulous/meticulous-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/alwaysmeticulous/meticulous-sdk.git",
    "directory": "packages/redaction-eslint-rules"
  },
  "bugs": {
    "url": "https://github.com/alwaysmeticulous/meticulous-sdk/issues"
  },
  "jest": {
    "preset": "../../jest.config.js"
  }
}
