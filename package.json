{
  "name": "meticulous-sdk",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "lerna run clean",
    "build": "turbo run build",
    "format": "lerna run format",
    "lint": "lerna run lint",
    "lint:commit": "lerna run lint:commit",
    "lint:fix": "lerna run lint:fix --no-bail",
    "test": "lerna run test",
    "cli": "yarn workspace @alwaysmeticulous/cli cli",
    "cli:dev": "yarn workspace @alwaysmeticulous/cli cli:dev",
    "cli:dev-localhost": "yarn workspace @alwaysmeticulous/cli cli:dev-localhost",
    "publish": "lerna version",
    "depcheck": "turbo run depcheck"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "depcheck": "^1.4.3",
    "esbuild": "^0.17.5",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^27.4.5",
    "lerna": "^7.1.0",
    "parcel": "^2.8.3",
    "prettier": "^2.7.1",
    "rimraf": "^4.0.7",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.8.1",
    "turbo": "^1.2.16",
    "typescript": "^4.9.4"
  },
  "version": "0.0.0"
}
