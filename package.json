{
  "name": "meticulous-sdk",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "turbo run clean",
    "build": "turbo run build",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "lint:commit": "turbo run lint:commit",
    "lint:fix": "turbo run lint:fix --no-bail",
    "test": "turbo run test",
    "cli": "pnpm --filter @alwaysmeticulous/cli cli",
    "cli:debug": "pnpm --filter @alwaysmeticulous/cli cli:debug",
    "cli:dev": "pnpm --filter @alwaysmeticulous/cli cli:dev",
    "cli:dev-staging": "pnpm --filter @alwaysmeticulous/cli cli:dev-staging",
    "cli:dev-localhost": "pnpm --filter @alwaysmeticulous/cli cli:dev-localhost",
    "publish": "lerna version",
    "depcheck": "turbo run depcheck"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@parcel/packager-ts": "^2.15.2",
    "@parcel/transformer-typescript-types": "^2.15.2",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "depcheck": "^1.4.3",
    "esbuild": "^0.25.4",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "lerna": "^9.0.0",
    "parcel": "^2.15.2",
    "prettier": "^3.5.3",
    "rimraf": "^4.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.8.1",
    "turbo": "^2.2.2",
    "typescript": "^4.9.4"
  },
  "pnpm": {
    "overrideNotes": {
      "json5": "https://nvd.nist.gov/vuln/detail/CVE-2022-46175"
    },
    "overrides": {
      "json5": "^2.2.2"
    }
  },
  "version": "0.0.0",
  "packageManager": "pnpm@10.27.0"
}
